{
    "name": "wearejust/user-bundle",
    "description": "Small wrapper for sonata user packages",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees van Egmond",
            "email": "cees.vanegmond@wearejust.com"
        },
        {
            "name": "John in 't Hout",
